{
  "compilerOptions": {
    "module": "CommonJS",
    "target": "ES6",
    "lib": [
      "ES6",
      "ES2020"
    ],
    "outDir": "./dist",
    "moduleResolution": "node",
    "rootDir": "./src",
    "noImplicitAny": true,
    "removeComments": true,
    "preserveConstEnums": true,
    "sourceMap": true,
    "strict": false
  }
}